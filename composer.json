    "description"      : "Yii2 support module. Ticket system with customer.",
    "homepage"         : "https://git.hexa.com.ua/yii2/support",
    "type"             : "module",
    "minimum-stability": "stable",
    "author"           : [
        {
            "name" : "Yevhenii Pylypenko",
            "email": "i.pylypenko@hexa.com.ua",
            "role" : "Developer"
